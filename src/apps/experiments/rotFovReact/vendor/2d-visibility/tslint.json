{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": [
            true,
            "spaces",
            4
        ],
        "quotemark": [
            true,
            "single"
        ],
        "member-ordering": [
            false
        ],
        "max-line-length": [
            true,
            200
        ],
        "ordered-imports": [
            false
        ],
        "object-literal-sort-keys": [
            false
        ]
    },
    "rulesDirectory": []
}