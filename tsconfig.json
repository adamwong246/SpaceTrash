{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["*"],
      "@public/*": ["public/*"],
      "@main/*": ["main/*"],
      "@lib/*": ["lib/*"],
      "@renderer*/": ["renderer/*"],
      "@utils/*": ["utils/*"],
      "@utilz/*": ["server/*"],
    },

    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitThis": false,
    "strictNullChecks": true,
    "sourceMap": true,
    "target": "esnext"
  },
  "exclude": [
    "node_modules",
    "trash"
  ]
}
